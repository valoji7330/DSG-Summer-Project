
/*package whatever //do not write package name here */
import java.io.*;

class GFG
{

// Direct XOR of all numbers from 1 to n
public static int computeXOR(int n)
{
	if (n % 4 == 0)
	return n;
	if (n % 4 == 1)
	return 1;
	if (n % 4 == 2)
	return n + 1;
	else
	return 0;
}

public static void main (String[] args) {

}
}

